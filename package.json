{
  "name": "danieljwagener.github.io",
  "version": "1.0.0",
  "description": "Daniel Wagener's portfolio website",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass scss/main.scss css/stylev2.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass scss/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/iconfont.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/stylev3.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielJWagener/DanielJWagener.github.io.git"
  },
  "author": "Daniel Wagener",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DanielJWagener/DanielJWagener.github.io/issues"
  },
  "homepage": "https://wwww.danieljwagener.com",
  "devDependencies": {
    "autoprefixer": "^9.7.6",
    "concat": "^1.0.3",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1"
  }
}
